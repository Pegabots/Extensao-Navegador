{
  "manifest_version": 3,
  "name": "BotBlocker",
  "description": "Extens√£o desenvolvida para o Hackathon do ITS - o melhor bot blocker",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": ["scripting", "contextMenus", "tabs", "activeTab"],
  "background": {
    "service_worker": "background/background.js",
    "default_icon": "icons/16.png",
    "type": "module"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}